{
  "name": "shopware/ci-test-repo",
  "description": "CI Test Repo",
  "type": "project",
  "require": {
    "shopware/core": "6.*",
    "shopware/storefront": "6.*"
  },
  "config": {
    "secure-http": false
  },
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "path",
      "url": "repos/core",
      "options": {
        "symlink": false
      }
    },
    {
      "type": "path",
      "url": "repos/storefront",
      "options": {
        "symlink": false
      }
    }
  ]
}
